a = ["Help me", "I'm here", "You lose"]
key = "123"
k = 1
choise = int(input("Введите индекс: "))
password = input("ведите пароль: ")


while password != key:
    if k == 3:
        break
    else:
        password = input("ведите пароль: ")
        k += 1
if password == key:
    if choise == 1:
        print(a[0])
    elif choise == 2:
        print(a[1])
    elif choise == 3:
        print(a[2])

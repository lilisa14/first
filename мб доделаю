import cv2
import numpy as np

b = np.zeros([700,700,3], np.uint8)   #black/white field
b[10:200, 10:300] = 178, 223, 74

cv2.rectangle(b,(50,40),(b.shape[0]//2,240),(0,0,155),thickness=2)
cv2.rectangle(b,(100,80),(390,280),(0,255,0),thickness=2)     #можно с thickness=cv2.FILLED

cv2.line(b,(10,10),(390,280),(255,255,255),thickness=2)

cv2.circle(b,(150,450),100,(255,0,0),thickness=2)

cv2.imshow("bs", b)
cv2.waitKey(0)
